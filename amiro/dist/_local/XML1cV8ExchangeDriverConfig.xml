<?xml version="1.0" encoding="windows-1251"?>
<!--DOCTYPE CONFIG SYSTEM "config.dtd"-->
<CONFIG>

    <!-- Имя тега, в котором находятся данные -->
    <START-TAG>v8msg:Body</START-TAG>

    <!-- Настройка удаления объектов -->
    <OBJECT-DELETION>
        <!-- Имя тега с информацией об удаленном объекте -->
        <TAG>ObjectDeletion</TAG>
        <OBJECT-ID>
        <!-- Имя тега с информацией об объекте -->
        <TAG>d2p1:Ref</TAG>
        <!-- Имя имя аттрибута с информацией о типе объекта -->
        <OBJECT-TYPE-ATTR>xsi:type</OBJECT-TYPE-ATTR>
        </OBJECT-ID>
    </OBJECT-DELETION>
    <DATE-FORMAT-STRING>YYYY-MM-DD hh:mm:ss</DATE-FORMAT-STRING>

    <!-- Объекты для экспорта
    <NAME-OBJECT-1C> Задает имя объекта в 1С
    <NAME-OBJECT> Задает имя (тип) объекта в CMS. Типы объектов строго фиксированы
    и задают порядок обработки. Для всех справочников, которые импортируются
    непосредственно в справочники ешопа указывается тип REFERENCE, тип справочника
    указывается в теге <DATA_TYPE>.
    В <ATTRIBS> перечисляем выгружаемые атрибуты, и устанавливаем им соответсвие
    нашим полям в теге <NAME>.
    По умолчанию, всегда выгружаются Ref, DeletionMark, Code, Description.
    -->
    <OBJECTS>
        <OBJECT>
            <!-- Справочник: валюты -->
            <OBJECT-INFO>
                <NAME-OBJECT-1C>CatalogObject.Валюты</NAME-OBJECT-1C>
                <OBJECT-DELETION-TYPE-1C>CatalogRef.Валюты</OBJECT-DELETION-TYPE-1C>
                <NAME-OBJECT>CURRENCY</NAME-OBJECT>
                <!-- имя тега, по которому определяется базовая валюта для интернет-магазина -->
                <BASE-CURRENCY-TAG>CODE</BASE-CURRENCY-TAG>
                <!-- значение тега, по которому определяется базовая валюта для интернет-магазина -->
                <BASE-CURRENCY-VALUE>810</BASE-CURRENCY-VALUE>
            </OBJECT-INFO>
            <ATTRIBS>
                <!-- индентификатор -->
                <ATTRIB>
                    <NAME-1C>Ref</NAME-1C>
                    <NAME>ID_EXTERNAL</NAME>
                </ATTRIB>
                <!-- помечен на удаление -->
                <ATTRIB>
                    <NAME-1C>DeletionMark</NAME-1C>
                    <NAME>IS_DELETED</NAME>
                </ATTRIB>
                <ATTRIB>
                    <NAME-1C>НаименованиеПолное</NAME-1C>
                    <NAME>NAME</NAME>
                </ATTRIB>
                <!-- стандартный код валюты -->
                <ATTRIB>
                    <NAME-1C>Code</NAME-1C>
                    <NAME>CODE</NAME>
                </ATTRIB>
                <!-- буквенный код валюты -->
                <ATTRIB>
                    <NAME-1C>Description</NAME-1C>
                    <NAME>DESCRIPTION</NAME>
                </ATTRIB>
                <!--ATTRIB>
                    <NAME-1C>ПараметрыПрописиНаРусском</NAME-1C>
                    <NAME></NAME>
                </ATTRIB-->
            </ATTRIBS>
        </OBJECT>
        <OBJECT>
            <!-- Курсы валют -->
            <OBJECT-INFO>
                <NAME-OBJECT-1C>InformationRegisterRecordSet.КурсыВалют</NAME-OBJECT-1C>
                <NAME-OBJECT>CURRENCY_RATE</NAME-OBJECT>
            </OBJECT-INFO>
            <ATTRIBS>
                <!-- Ссылка на справочник валют -->
                <ATTRIB>
                    <NAME-1C>Валюта</NAME-1C>
                    <NAME>ID_EXTERNAL</NAME>
                </ATTRIB>
                <ATTRIB>
                    <NAME-1C>Курс</NAME-1C>
                    <NAME>RATE</NAME>
                </ATTRIB>
                <ATTRIB>
                    <NAME-1C>Period</NAME-1C>
                    <NAME>DATE</NAME>
                </ATTRIB>
                <!--ATTRIB>
                    <NAME-1C>Кратность</NAME-1C>
                    <NAME></NAME>
                </ATTRIB-->
            </ATTRIBS>
        </OBJECT>
        <OBJECT>
            <!-- Справочник: тип цен -->
            <OBJECT-INFO>
                <NAME-OBJECT-1C>CatalogObject.ТипыЦенНоменклатуры</NAME-OBJECT-1C>
                <NAME-OBJECT>PRICE_TYPE</NAME-OBJECT>
                <ALWAYS>True</ALWAYS>
                <!-- Имя тега, по которому осуществляется соответсвие цен -->
                <PRICE-ID-TAG>CODE</PRICE-ID-TAG>
                <PRICES-MAP>
                    <!-- Карта соответсвий цен -->
                    <!-- Тег <VALUE-1C> задает значение 1С определяющего тега цен (<PRICE-ID-TAG>),
                         тег <PRICE-NUMBER> задает номер цены интернет-магазина,
                         <ID-EXTERNAL> - обязательный тег (только для импорта в 1С), задает UUID типа цены 1С
                    -->
                    <MAP>
                        <VALUE-1C>00001</VALUE-1C>
                        <PRICE-NUMBER>1</PRICE-NUMBER>
                        <!--ID-EXTERNAL>dfd95abe-63b9-11d9-a785-505054503030</ID-EXTERNAL-->
                        <ID-EXTERNAL>b7279e33-9af1-11da-966f-0001029e9b10</ID-EXTERNAL>
                    </MAP>
                    <MAP>
                        <VALUE-1C>00002</VALUE-1C>
                        <PRICE-NUMBER>2</PRICE-NUMBER>
                        <!--ID-EXTERNAL>b1ecf2d2-63b2-11d9-a785-505054503030</ID-EXTERNAL-->
                        <ID-EXTERNAL>b7279e34-9af1-11da-966f-0001029e9b10</ID-EXTERNAL>
                    </MAP>
                    <!--MAP>
                        <VALUE-1C>00003</VALUE-1C>
                        <PRICE-NUMBER>3</PRICE-NUMBER>
                        <ID-EXTERNAL>b1ecf2d4-63b2-11d9-a785-505054503030</ID-EXTERNAL>
                    </MAP>
                    <MAP>
                        <VALUE-1C>00004</VALUE-1C>
                        <PRICE-NUMBER>4</PRICE-NUMBER>
                        <ID-EXTERNAL>23051ccf-9866-11d9-9b59-0001029e9b10</ID-EXTERNAL>
                    </MAP>
                    <MAP>
                        <VALUE-1C>00005</VALUE-1C>
                        <PRICE-NUMBER>5</PRICE-NUMBER>
                        <ID-EXTERNAL>30cc35ad-63b3-11d9-a785-505054503030</ID-EXTERNAL>
                    </MAP>
                    <MAP>
                        <VALUE-1C>00006</VALUE-1C>
                        <PRICE-NUMBER>6</PRICE-NUMBER>
                        <ID-EXTERNAL>23051cd1-9866-11d9-9b59-0001029e9b10</ID-EXTERNAL>
                    </MAP-->
                </PRICES-MAP>
                <TAXES-MAP>
                    <!-- Карта соответсвия налогов, и их значений -->
                    <!-- Тег <VALUE-1C> задает значение налога 1С,
                         тег <PERCENT> задает абсолютное или арифметическое выражение налога 
                    -->
                    <MAP>
                        <VALUE-1C>НДС18</VALUE-1C>
                        <PERCENT>18</PERCENT>
                    </MAP>
                    <MAP>
                        <VALUE-1C>НДС18_118</VALUE-1C>
                        <PERCENT>18/118*100</PERCENT>
                    </MAP>
                    <MAP>
                        <VALUE-1C>НДС10</VALUE-1C>
                        <PERCENT>10</PERCENT>
                    </MAP>
                    <MAP>
                        <VALUE-1C>НДС10_110</VALUE-1C>
                        <PERCENT>10/110*100</PERCENT>
                    </MAP>
                    <MAP>
                        <VALUE-1C>НДС0</VALUE-1C>
                        <PERCENT>0</PERCENT>
                    </MAP>
                    <MAP>
                        <VALUE-1C>БезНДС</VALUE-1C>
                        <PERCENT>0</PERCENT>
                    </MAP>
                    <MAP>
                        <VALUE-1C>НДС20</VALUE-1C>
                        <PERCENT>20</PERCENT>
                    </MAP>
                    <MAP>
                        <VALUE-1C>НДС20_120</VALUE-1C>
                        <PERCENT>20/120*100</PERCENT>
                    </MAP>
                </TAXES-MAP>
            </OBJECT-INFO>
            <ATTRIBS>
                <ATTRIB>
                    <NAME-1C>Ref</NAME-1C>
                    <NAME>ID_EXTERNAL</NAME>
                </ATTRIB>
                <ATTRIB>
                    <NAME-1C>Code</NAME-1C>
                    <NAME>CODE</NAME>
                </ATTRIB>
                <ATTRIB>
                    <NAME-1C>DeletionMark</NAME-1C>
                    <NAME>IS_DELETED</NAME>
                </ATTRIB>
                <ATTRIB>
                    <NAME-1C>Description</NAME-1C>
                    <NAME>DESCRIPTION</NAME>
                </ATTRIB>
                <ATTRIB>
                    <NAME-1C>ВалютаЦены</NAME-1C>
                    <NAME>CURRENCY_ID_EXTERNAL</NAME>
                </ATTRIB>
                <!--ATTRIB>
                    <NAME-1C>БазовыйТипЦен</NAME-1C>
                    <NAME></NAME>
                </ATTRIB-->
                <!--ATTRIB>
                    <NAME-1C>Рассчитывается</NAME-1C>
                    <NAME></NAME>
                </ATTRIB-->
                <ATTRIB>
                    <NAME-1C>ПроцентСкидкиНаценки</NAME-1C>
                    <NAME>DISCOUNT</NAME>
                </ATTRIB>
                <ATTRIB>
                    <NAME-1C>ЦенаВключаетНДС</NAME-1C>
                    <NAME>WITH_TAX</NAME>
                </ATTRIB>
                <!--ATTRIB>
                    <NAME-1C>ПорядокОкругления</NAME-1C>
                    <NAME></NAME>
                </ATTRIB-->
                <!--ATTRIB>
                    <NAME-1C>ОкруглятьВБольшуюСторону</NAME-1C>
                    <NAME></NAME>
                </ATTRIB-->
            </ATTRIBS>
        </OBJECT>
        <OBJECT>
            <!-- Регистр: цены -->
            <OBJECT-INFO>
                <NAME-OBJECT-1C>InformationRegisterRecordSet.ЦеныНоменклатуры</NAME-OBJECT-1C>
                <NAME-OBJECT>PRICE</NAME-OBJECT>
            </OBJECT-INFO>
            <ATTRIBS>
                <!-- дата -->
                <ATTRIB>
                    <NAME-1C>Period</NAME-1C>
                    <NAME>DATE</NAME>
                </ATTRIB>
                <!-- признак активности -->
                <ATTRIB>
                    <NAME-1C>Active</NAME-1C>
                    <NAME>ACTIVE</NAME>
                </ATTRIB>
                <!-- ссылка на тип цены -->
                <ATTRIB>
                    <NAME-1C>ТипЦен</NAME-1C>
                    <NAME>TYPE_PRICE_ID_EXTERNAL</NAME>
                </ATTRIB>
                <!-- ссылка на номенклатуру -->
                <ATTRIB>
                    <NAME-1C>Номенклатура</NAME-1C>
                    <NAME>ITEM_ID_EXTERNAL</NAME>
                </ATTRIB>
                <!-- ссылка на валюту -->
                <ATTRIB>
                    <NAME-1C>Валюта</NAME-1C>
                    <NAME>CURRENCY_ID_EXTERNAL</NAME>
                </ATTRIB>
                <!-- цена -->
                <ATTRIB>
                    <NAME-1C>Цена</NAME-1C>
                    <NAME>PRICE</NAME>
                </ATTRIB>
                <!--ATTRIB>
                    <NAME-1C>ЕдиницаИзмерения</NAME-1C>
                    <NAME>UNIT</NAME>
                </ATTRIB-->
                <ATTRIB>
                    <NAME-1C>ПроцентСкидкиНаценки</NAME-1C>
                    <NAME>DISCOUNT</NAME>
                </ATTRIB>
            </ATTRIBS>
            </OBJECT>
            <OBJECT>
                <!-- Справочник:  Классификатор единиц измерения -->
                <OBJECT-INFO>
                    <NAME-OBJECT-1C>CatalogObject.КлассификаторЕдиницИзмерения</NAME-OBJECT-1C>
                    <OBJECT-DELETION-TYPE-1C>CatalogRef.КлассификаторЕдиницИзмерения</OBJECT-DELETION-TYPE-1C>
                    <NAME-OBJECT>REFERENCE</NAME-OBJECT>
                    <DATA_TYPE>UNIT</DATA_TYPE>
                </OBJECT-INFO>
                <ATTRIBS>
                    <ATTRIB>
                        <NAME-1C>Ref</NAME-1C>
                        <NAME>ID_EXTERNAL</NAME>
                    </ATTRIB>
                    <ATTRIB>
                        <NAME-1C>Description</NAME-1C>
                        <NAME>UNIT_NAME</NAME>
                    </ATTRIB>
                </ATTRIBS>
            </OBJECT>
        <OBJECT>
            <!-- Справочник:  Классификатор стран мира -->
            <OBJECT-INFO>
                <NAME-OBJECT-1C>CatalogObject.КлассификаторСтранМира</NAME-OBJECT-1C>
                <NAME-OBJECT>REFERENCE</NAME-OBJECT>
                <DATA_TYPE>COUNTRY</DATA_TYPE>
            </OBJECT-INFO>
            <ATTRIBS>
                <ATTRIB>
                    <NAME-1C>Ref</NAME-1C>
                    <NAME>ID_EXTERNAL</NAME>
                </ATTRIB>
                <ATTRIB>
                    <NAME-1C>Description</NAME-1C>
                    <NAME>COUNTRY_NAME</NAME>
                </ATTRIB>
                <ATTRIB>
                    <NAME-1C>НаименованиеПолное</NAME-1C>
                    <NAME>COUNTRY_FULL_NAME</NAME>
                </ATTRIB>
            </ATTRIBS>
        </OBJECT>
        <OBJECT>
            <!-- Справочник:  Номенклатура -->
            <OBJECT-INFO>
                <NAME-OBJECT-1C>CatalogObject.Номенклатура</NAME-OBJECT-1C>
                <NAME-OBJECT>CATALOG</NAME-OBJECT>
                <OBJECT-DELETION-TYPE-1C>CatalogRef.Номенклатура</OBJECT-DELETION-TYPE-1C>
            </OBJECT-INFO>
            <!-- выгружаемые реквизиты -->
            <ATTRIBS>
                <!-- индентификатор -->
                <ATTRIB>
                    <NAME-1C>Ref</NAME-1C>
                    <NAME>ID_EXTERNAL</NAME>
                </ATTRIB>
                <!-- признак: группа/товар -->
                <ATTRIB>
                    <NAME-1C>IsFolder</NAME-1C>
                    <NAME>IS_FOLDER</NAME>
                </ATTRIB>
                <!-- признак: помечен на удаление -->
                <ATTRIB>
                    <NAME-1C>DeletionMark</NAME-1C>
                    <NAME>IS_DELETED</NAME>
                </ATTRIB>
                <!-- ссылка на родительский элемент -->
                <ATTRIB>
                    <NAME-1C>Parent</NAME-1C>
                    <NAME>ID_PARENT_EXTERNAL</NAME>
                </ATTRIB>
                <ATTRIB>
                    <NAME-1C>Code</NAME-1C>
                    <NAME>CODE</NAME>
                </ATTRIB>
                <ATTRIB>
                    <NAME-1C>Description</NAME-1C>
                    <NAME>DESCRIPTION</NAME>
                </ATTRIB>
                <ATTRIB>
                    <NAME-1C>НаименованиеПолное</NAME-1C>
                    <NAME>NAME_FULL</NAME>
                </ATTRIB>
                <ATTRIB>
                    <NAME-1C>Артикул</NAME-1C>
                    <NAME>ARTIKUL</NAME>
                </ATTRIB>
                <!-- ссылка на номер ГТД -->
                <ATTRIB>
                    <NAME-1C>НомерГТД</NAME-1C>
                    <NAME>GTD_NUMBER</NAME>
                </ATTRIB>
                <!-- ссылка на страну происхождения -->
                <ATTRIB>
                    <NAME-1C>СтранаПроисхождения</NAME-1C>
                    <NAME>COUNTRY_NAME</NAME>
                </ATTRIB>
                <!-- ссылка на базоваю единицу измерения -->
                <ATTRIB>
                    <NAME-1C>БазоваяЕдиницаИзмерения</NAME-1C>
                    <NAME>UNIT_NAME</NAME>
                </ATTRIB>
                <ATTRIB>
                    <NAME-1C>СтавкаНДС</NAME-1C>
                    <NAME>TAX</NAME>
                </ATTRIB>
            </ATTRIBS>
        </OBJECT>
        <OBJECT>
            <!-- справочник доп. информации -->
            <OBJECT-INFO>
                <NAME-OBJECT-1C>CatalogObject.ХранилищеДополнительнойИнформации</NAME-OBJECT-1C>
                <NAME-OBJECT>IMAGE</NAME-OBJECT>
                <!-- тип элемента по умолчанию -->
                <DEFAULT_TYPE>ОсновноеИзображение</DEFAULT_TYPE>
                <!-- имя элемента, в котором задается тип объекта (изображения) -->
                <TYPE_TAG>IMAGE_TYPE</TYPE_TAG>
                <TYPES>
                    <!-- карта соответсвий типов объектов (изображений) -->
                    <!-- тег <FILE_EXTENSION> задает расширения файла по умолчанию -->
                    <!-- значение тегов <NAME> изменять нельзя -->
                    <!-- описания значений тега <NAME>:
                      MAIN  - главное изображение товара
                      POPUP - большое изображение товара
                      SMALL - маленькое изображение товара
                      FILE  - файл товара (digital)
                    -->
                    <!-- для значения FILE, допустим атрибут  type="free",
                         который определяет файлы данного типа, как свободные
                         для скачивания (бесплатные)
                    -->
                    <TYPE>
                        <VALUE-1C>ОсновноеИзображение</VALUE-1C>
                        <NAME>MAIN</NAME>
                        <FILE_EXTENSION>jpg</FILE_EXTENSION>
                    </TYPE>
                    <TYPE>
                        <VALUE-1C>БольшоеИзображение</VALUE-1C>
                        <NAME>POPUP</NAME>
                        <FILE_EXTENSION>jpg</FILE_EXTENSION>
                    </TYPE>
                    <TYPE>
                        <VALUE-1C>МаленькоеИзображение</VALUE-1C>
                        <NAME>SMALL</NAME>
                        <FILE_EXTENSION>jpg</FILE_EXTENSION>
                    </TYPE>
                    <TYPE>
                        <VALUE-1C>Инструкция</VALUE-1C>
                        <NAME type="free">FILE</NAME>
                        <FILE_EXTENSION>doc</FILE_EXTENSION>
                    </TYPE>
                    <TYPE>
                        <VALUE-1C>ФайлMp3</VALUE-1C>
                        <NAME type="free">FILE</NAME>
                        <FILE_EXTENSION>mp3</FILE_EXTENSION>
                    </TYPE>
                </TYPES>
            </OBJECT-INFO>
            <ATTRIBS>
                <ATTRIB>
                    <NAME-1C>ТипДанных</NAME-1C>
                    <NAME>IMAGE_TYPE</NAME>
                </ATTRIB>
                <ATTRIB>
                    <NAME-1C>Ref</NAME-1C>
                    <NAME>ID_EXTERNAL</NAME>
                </ATTRIB>
                <ATTRIB>
                    <NAME-1C>DeletionMark</NAME-1C>
                    <NAME>IS_DELETED</NAME>
                </ATTRIB>
                <ATTRIB>
                    <NAME-1C>Description</NAME-1C>
                    <NAME>DESCRIPTION</NAME>
                </ATTRIB>
                <ATTRIB>
                    <NAME-1C>ИмяФайла</NAME-1C>
                    <NAME>FILE_NAME</NAME>
                </ATTRIB>
                <!-- ссылка на объект номенклатуры -->
                <ATTRIB>
                    <NAME-1C>Объект</NAME-1C>
                    <NAME>ID_OBJECT_EXTERNAL</NAME>
                </ATTRIB>
                <ATTRIB>
                    <NAME-1C type="File" name="Ref">Хранилище</NAME-1C>
                    <NAME>FILE</NAME>
                </ATTRIB>
            </ATTRIBS>
        </OBJECT>
    </OBJECTS>

    <!-- раздел, параметры архивирования -->
    <COMPRESSION>
        <IS-COMRESSION>False</IS-COMRESSION>
        <NAME-ARCHIVE>arch</NAME-ARCHIVE>
        <PASSWD>Пароль</PASSWD>
    </COMPRESSION>
    <!-- раздел, параметры кодирования данных -->
    <ENCODING>
        <!-- Тег <TYPE-1C> - задает кодировку выходных данных 1С, допустимые значения:
              utf8 - unicode
              ansi - кодировка windows
              oem  - DOS кодировка
        -->
        <!-- Тег <PARSER> задает входную кодировку данных для парсера XML (в текущей версии доступно только utf-8)-->
        <!-- Тег <FROM> задает входную кодировку данных для перекодировщика (в текущей версии доступно только utf-8)-->
        <!-- Тег <TO> задает выходную кодировку данных для перекодировщика (в текущей версии доступно только windows-1251)-->
        <TYPE-1C>utf8</TYPE-1C>  
        <FROM>utf-8</FROM>
        <TO>windows-1251</TO>
        <PARSER>utf-8</PARSER>
    </ENCODING>

    <!-- раздел, параметры FTP -->
    <FTP>
        <!-- отправлять (True), или нет (False) выгруженные данные на сервер -->
        <FTP-ENABLE>False</FTP-ENABLE>
        <FTP-SERVER>wmtest.nsknet.ru</FTP-SERVER>
        <FTP-LOGIN>wmtest</FTP-LOGIN>
        <FTP-PASSWD>testNETmanager32</FTP-PASSWD>
        <FTP-TYPE-CONNECTION>True</FTP-TYPE-CONNECTION>
        <FTP-DIRECTORY>/alena/public_html/In/</FTP-DIRECTORY>
    </FTP>

    <!-- раздел, параметры сохранения файлов -->
    <ADDIN_FILES>
        <!-- расширение по умолчанию для сохраняемых файлов -->
        <EXTENSION>dat</EXTENSION>
    </ADDIN_FILES>

    <DELETION-CHANGES>
      <TYPE-DELETION>at_once</TYPE-DELETION>
    </DELETION-CHANGES>

    <!-- раздел, входящие сообщения -->
    <MESSAGES>
        <!-- разрешить удаление удачно обработанных сообщений -->
        <DELETE-MESSAGES>True</DELETE-MESSAGES> 
    </MESSAGES>

    <!-- раздел, параметры сервера pop3 -->
    <MAIL>
        <!-- принимать информацию с e-mail -->
        <MAIL-ENABLE>False</MAIL-ENABLE>
        <!-- имя пользователя сервера pop3 -->
        <MAIL-LOGIN>karzhenkov-test</MAIL-LOGIN>
        <!-- имя сервера pop3 -->
        <MAIL-POP3-SERVER>pop.yandex.ru</MAIL-POP3-SERVER>
        <!-- пароль пользователя сервера pop3 -->
        <MAIL-PASSWD></MAIL-PASSWD>
    </MAIL>

    <!-- раздел, отладка -->
    <DEBUG>
        <!-- включение/выключение режима отладки -->
        <LOG_ALL_OBJECTS>False</LOG_ALL_OBJECTS>
        <!-- имя файла для записи отладочно информации -->
        <NAME_LOG_FILE>import_debug.log.xml</NAME_LOG_FILE>
    </DEBUG>
</CONFIG>